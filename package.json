{
  "name": "electron-ad-blocker",
  "version": "0.0.0-dev.0",
  "description": "Simple util to block ads in an electron browser window",
  "repository": "https://github.com/Jense5/electron-ad-blocker.git",
  "author": "Jensen Bernard <jensenbernard5@gmail.com> (twitter.com/Jense5_)",
  "license": "MPL",
  "keywords": [ "electron-ad-blocker" ],
  "files": [ "dist", "README.md" ],
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist --copy-files",
    "build:watch": "npm run build && npm run build -- -w",
    "eslint": "eslint src test",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "pretest": "npm run eslint && npm run flow",
    "test": "mocha test --compilers js:babel-core/register",
    "test:watch": "npm run test -- -w",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-env": "^1.1.8",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.37.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
